<?xml version="1.0" encoding="utf-8" ?>
<manifest xmlns="http://oberon.thehomeofjon.net"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://oberon.thehomeofjon.net/bin/xsd/manifest-v1.xsd">
    <plugins>
        <plugin>
            <name>Banned Words</name>
            <description>
                <![CDATA[Plugin that allows operator to kick users who use banned words within the chatroom.]]>
            </description>
            <author>bigmanhaywood</author>
            <homepageUrl>http://oberon.thehomeofjon.net/plugins/extras/</homepageUrl>
        </plugin>
        <plugin>
            <name>Calc</name>
            <description>
                <![CDATA[Class the performs simple math calculations.]]>
            </description>
            <author>bigmanhaywood</author>
            <homepageUrl>http://oberon.thehomeofjon.net/plugins/extras/</homepageUrl>
        </plugin>
        <plugin>
            <name>Dictionary</name>
            <description>
                <![CDATA[Plugin that lets a user find the definition of a word.]]>
            </description>
            <author>bigmanhaywood</author>
            <homepageUrl>http://oberon.thehomeofjon.net/plugins/extras/</homepageUrl>
        </plugin>
        <plugin>
            <name>Games</name>
            <description>
                <![CDATA[Small chat room games.]]>
            </description>
            <author>bigmanhaywood</author>
            <homepageUrl>http://oberon.thehomeofjon.net/plugins/extras/</homepageUrl>
        </plugin>
        <plugin>
            <name>Regex</name>
            <description>
                <![CDATA[Plugin that lets a user test a regex pattern.]]>
            </description>
            <author>bigmanhaywood</author>
            <homepageUrl>http://oberon.thehomeofjon.net/plugins/extras/</homepageUrl>
        </plugin>
        <plugin>
            <name>Responses</name>
            <description>
                <![CDATA[This is a bot response module for Oberon.]]>
            </description>
            <author>bigmanhaywood</author>
            <homepageUrl>http://oberon.thehomeofjon.net/plugins/extras/</homepageUrl>
        </plugin>
        <plugin>
            <name>Spell Checker</name>
            <description>
                <![CDATA[Plugin that checks the proper spelling of a word.]]>
            </description>
            <author>bigmanhaywood</author>
            <homepageUrl>http://oberon.thehomeofjon.net/plugins/extras/</homepageUrl>
        </plugin>
        <plugin>
            <name>VisitCount</name>
            <description>
                <![CDATA[Plugin to keep track of new username's seen in chatroom since bot logs in.]]>
            </description>
            <author>bigmanhaywood</author>
            <homepageUrl>http://oberon.thehomeofjon.net/plugins/extras/</homepageUrl>
        </plugin>
    </plugins>
    <version>1.0.1.0</version>
    <updates>
        <!-- The min and max bot version this plugin is compatible with -->
        <minBotVersion>0.1</minBotVersion>
        <maxBotVersion>0.8.9</maxBotVersion>

        <!-- This is a text file which contains the latest version of your plugin -->
        <updateManifestUrl>http://oberon.thehomeofjon.net/bin/plugins/extras/manifest.xml</updateManifestUrl>

        <!-- This is where this version of the plugin will be downloaded from -->
        <updateUrl>http://oberon.thehomeofjon.net/bin/plugins/extras/extras.zip</updateUrl>
    </updates>
</manifest>